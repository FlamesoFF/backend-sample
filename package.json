{
  "name": "apollo-api",
  "version": "0.96.1",
  "private": true,
  "scripts": {
    "serve": "tsc --project tsconfig.dev.json",
    "test-unit": "mocha -c tests/.mocharc.json --require ts-node/register tests/**/*.unit.test.ts",
    "test-integration": "mocha -c tests/.mocharc.json --require ts-node/register tests/**/*.integration.test.ts",
    "test-integration-external": "mocha -c tests/.mocharc.json --require ts-node/register tests/**/external.*.integration.test.ts",
    "test-api-integration": "jest --clearCache && jest",
    "test-sync-integration": "jest --clearCache && jest tests/synchronizer/integration/integration.test.ts",
    "v3-format-orders": "ts-node utils/orders/format.ts",
    "fill-db-dummy-data": "ts-node utils/dummy-data/fill-db.ts --config config-local.json",
    "initialize-pgsql": "ts-node init-pgsql.ts",
    "start-syncr": "pm2 start ./src/synchronizer/main.ts --interpreter ts-node --name \"Synchronizer\" -- --init",
    "start-syncr-docker": "ts-node -P tsconfig.json ./src/synchronizer/main.ts --init",
    "start-syncr-no-init": "pm2 start ./src/synchronizer/main.ts --interpreter ts-node --name \"Synchronizer\"",
    "start-syncr-dev": "ts-node -P tsconfig.json ./src/synchronizer/main.ts --init --config config-local.json",
    "start-syncr-dev-no-init": "ts-node -P tsconfig.json ./src/synchronizer/main.ts --config config-local.json",
    "start-api-dev": "ts-node -P tsconfig.json ./src/api/main.ts --config config-local.json",
    "start-api-docker": "ts-node -P tsconfig.json ./src/api/main.ts",
    "start-api": "pm2 start pm2-api.json",
    "logs": "pm2 logs \"API v3\"",
    "stop": "pm2 stop \"API v3\" && pm2 delete \"API v3\""
  },
  "dependencies": {
    "@apollo4u/auxiliary": "4.0.0",
    "ajv": "6.12.0",
    "axios": "^0.19.2",
    "body-parser": "1.19.0",
    "check-types": "^11.0.0",
    "core-js": "^3.4.5",
    "cors": "2.8.5",
    "cross-env": "^7.0.2",
    "crypto": "1.0.1",
    "debug": "4.1.1",
    "elasticsearch": "^16.4.0",
    "express": "5.0.0-alpha.8",
    "faker": "^4.1.0",
    "google-auth-library": "^6.0.0",
    "googleapis": "^48.0.0",
    "install": "^0.12.2",
    "json-schema-faker": "^0.5.0-rcv.24",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "nano": "^8.2.2",
    "nconf": "^0.10.0",
    "neo4j-cypher-query-builder": "0.1.4",
    "neo4j-driver": "4.0.2",
    "numeral": "^2.0.6",
    "pg": "8.0.3",
    "randexp": "^0.5.3",
    "sequelize": "^5.21.11",
    "sha.js": "^2.4.11",
    "spdy": "^4.0.2",
    "statuses": "1.5.0",
    "swagger-jsdoc": "3.4.0",
    "ts-node": "^8.5.4",
    "typescript": "3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-class-properties": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/ajv": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.11",
    "@types/cors": "2.8.5",
    "@types/debug": "4.1.5",
    "@types/elasticsearch": "^5.0.36",
    "@types/expect": "^24.3.0",
    "@types/express": "4.17.1",
    "@types/faker": "^4.1.11",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.36",
    "@types/multer": "^1.4.3",
    "@types/mysql": "^2.15.8",
    "@types/nconf": "^0.10.0",
    "@types/node": "12.11.7",
    "@types/numeral": "0.0.28",
    "@types/pg": "7.11.1",
    "@types/request": "^2.48.4",
    "@types/sequelize": "^4.28.9",
    "@types/sha.js": "^2.4.0",
    "@types/spdy": "^3.4.4",
    "@types/statuses": "1.5.0",
    "@types/webpack": "4.39.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/typescript-estree": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "got": "^11.3.0",
    "mocha": "^8.0.1"
  },
  "jest-runner-tsc": {
    "tsconfigPath": "./tsconfig.json"
  }
}
