{
  "name": "apollo-api",
  "version": "0.105.0",
  "private": true,
  "scripts": {
    "serve": "tsc --project tsconfig.dev.json",
    "test-unit": "ts-mocha **/*.unit.spec.ts",
    "test-unit:coverage": "ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./test-reports/junit.xml **/*unit.spec.ts",
    "test-integration": "ts-mocha --exit **/*int.spec.ts",
    "test-integration:coverage": "ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./test-reports/junit.xml --exit **/*.int.spec.ts",
    "v3-format-orders": "ts-node utils/orders/transform.ts",
    "fill-db-dummy-data": "ts-node  utils/dummy-data/fill-db.ts --config config.local.json",
    "initialize-pgsql": "ts-node  database/pgsql/init.ts",
    "initialize-cdb": "ts-node database/couchdb/init.ts",
    "initialize-neo4j": "ts-node database/neo4j/init.ts",
    "start-syncr": "ts-node ./src/synchronizer/main.ts --init",
    "start-syncr-dev": "ts-node ./src/synchronizer/main.ts --init",
    "start-syncr-dev-no-init": "ts-node ./src/synchronizer/main.ts --config config.local.json",
    "start-api": "ts-node ./src/api/main.ts"
  },
  "dependencies": {
    "@apollo4u/auxiliary": "4.0.0",
    "@graphql-tools/graphql-file-loader": "^6.0.18",
    "@graphql-tools/load": "^6.0.18",
    "@graphql-tools/schema": "^6.0.18",
    "@graphql-tools/utils": "^6.0.18",
    "ajv": "6.12.0",
    "apollo-server-express": "^2.16.1",
    "cross-env": "7.0.2",
    "crypto": "1.0.1",
    "elasticsearch": "16.4.0",
    "express": "5.0.0-alpha.8",
    "express-graphql": "^0.11.0",
    "faker": "4.1.0",
    "google-auth-library": "6.0.0",
    "googleapis": "48.0.0",
    "got": "11.5.2",
    "graphql": "^15.3.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "mysql": "2.17.1",
    "nano": "8.2.2",
    "nconf": "0.10.0",
    "neo4j-driver": "4.0.2",
    "numeral": "2.0.6",
    "pg": "8.0.3",
    "randexp": "^0.5.3",
    "spdy": "4.0.2",
    "statuses": "1.5.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/elasticsearch": "5.0.36",
    "@types/express": "4.17.1",
    "@types/faker": "4.1.11",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "8.0.1",
    "@types/multer": "1.4.3",
    "@types/mysql": "2.15.8",
    "@types/nconf": "0.10.0",
    "@types/node": "14.0.27",
    "@types/numeral": "0.0.28",
    "@types/pg": "7.11.1",
    "@types/spdy": "3.4.4",
    "@types/statuses": "1.5.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "@typescript-eslint/typescript-estree": "2.30.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "mocha": "7.2.0",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "ts-mocha": "7.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "ts"
    ],
    "check-leaks": true,
    "reporter": "spec",
    "timeout": 2000,
    "ui": "bdd"
  }
}
